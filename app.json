{
  "expo": {
    "name": "reachout-app",
    "slug": "reachout-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./src/index.tsx",
    "icon": "./src/assets/img/apple-icon.png",
    "splash": {
      "image": "./src/assets/img/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/assets/img/favicon.png"
    }
  }
}
